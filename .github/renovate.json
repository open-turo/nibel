{
  "extends": [
    "github>open-turo/renovate-config#v1.14.0",
    "github>open-turo/renovate-config:jvm#v1.14.0"
  ],
  "labels": [
    "dependencies"
  ],
  "prConcurrentLimit": 10,
  "packageRules": [
    {
      "groupName": "kotlin",
      "matchPackageNames": [
        "/androidx.compose.compiler:compiler/"
      ]
    },
    {
      "groupName": "kotlin",
      "matchPackageNames": [
        "/org.jetbrains.kotlin.*/"
      ]
    },
    {
      "groupName": "kotlin",
      "matchPackageNames": [
        "/com.google.devtools.ksp/"
      ]
    }
  ]
}
